<h1>Welcome, <%= @user.username %></h1>

<h3>Your Travel Destination Wishlist</h3>

<% @wished_places.each do |wish| %>
  <input type="checkbox" name="wished_place" value="<%=wish.id%>"><%=wish.destination%></input>
<% end %>

<br>

<h3>Places You've Been Around the World</h3>

<ul>
  <% @visited_places.each do |visit| %>
    <li><%= visit.destination %></li>
    <br>
  <% end %>
</ul>
